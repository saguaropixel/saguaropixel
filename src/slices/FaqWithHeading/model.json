{
  "type": "SharedSlice",
  "id": "faq_with_heading",
  "name": "FaqWithHeading",
  "variations": [
    {
      "id": "default",
      "name": "Default",
      "description": "Displays a heading followed by a vertically stacked list of FAQs, each with question and optional answer.",
      "version": "initial",
      "imageUrl": "",
      "docURL": "...",
      "primary": {
        "background_color": {
          "type": "Link",
          "config": {
            "label": "Background Color",
            "select": "document",
            "repeat": false,
            "customtypes": [
              {
                "id": "brand_color",
                "fields": [
                  "name",
                  "value"
                ]
              }
            ]
          }
        },
        "heading": {
          "type": "StructuredText",
          "config": {
            "single": "heading2",
            "label": "Heading"
          }
        },
        "faq_items": {
          "type": "Group",
          "config": {
            "label": "FAQ Items",
            "fields": {
              "question": {
                "type": "StructuredText",
                "config": {
                  "label": "Question",
                  "placeholder": "",
                  "allowTargetBlank": true,
                  "single": "paragraph,strong,em"
                }
              },
              "answer": {
                "type": "StructuredText",
                "config": {
                  "multi": "paragraph,strong,em,hyperlink",
                  "label": "Answer"
                }
              }
            }
          }
        }
      },
      "items": {}
    }
  ]
}